{
  "name": "crud_with_mocha",
  "version": "1.0.0",
  "description": "CRUD with express and mocha",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --timeout 10000 --exit",
    "test-coverage": "nyc npm test",
    "report-coverage": "nyc --reporter=lcov --reporter=text-lcov npm test && chromium-browser coverage/lcov-report/index.html",
    "coverage": "npm run test-coverage --report && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [],
  "author": "ThiaguinhoLS",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "express": "^4.16.4",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "use-strict": "^1.0.1"
  }
}
